{
  "author": "Arian Acosta (https://twitter.com/arian3k)",
  "bin": "bin/install.js",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "fs-extra": "^9.0.0",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3",
    "yargs": "^15.3.1"
  },
  "description": "Automatically install and maintain your code style with linters and formatters for JavaScript, TypeScript and React.",
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.3",
    "husky": "^4.0.7",
    "lint-staged": "^9.5.0",
    "semantic-release": "^16.0.1",
    "semantic-release-npm-github-publish": "^1.4.0"
  },
  "homepage": "https://github.com/arianacosta/poetic",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "poetic",
    "style",
    "linter",
    "formatter",
    "eslint",
    "tslint",
    "javascript",
    "typescript",
    "prettier",
    "airbnb"
  ],
  "license": "MIT",
  "lint-staged": {
    "extends": []
  },
  "main": "config/index.js",
  "name": "poetic",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arianacosta/poetic.git"
  },
  "scripts": {
    "cz": "git-cz",
    "develop": "node scripts/develop.js",
    "upgrade:dependencies": "npx npm-check-updates -u"
  },
  "version": "1.5.0"
}
